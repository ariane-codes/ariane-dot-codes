{
  "name": "blog-nextjs",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:sanity": "cd ../studio && npm install && sanity build ../blog-nextjs/public/studio -y && cd ../blog-nextjs",
    "build:web": "next build",
    "start:web": "next start",
    "start:sanity": "cp .env.local ../studio/.env.development && cd ../studio && sanity start",
    "lint": "next lint",
    "cypress": "cypress open",
    "cypress:headless": "cypress run"
  },
  "dependencies": {
    "@sanity/client": "^2.23.2",
    "classnames": "^2.3.1",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@sanity/cli": "^2.0.5",
    "cypress": "^9.1.1",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "lerna": "^4.0.0",
    "postcss": "^8.4.5"
  },
  "peerDependencies": {
    "postcss": "^8.4.5"
  }
}
